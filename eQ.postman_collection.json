		{
			"name": "Sign Up",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "32c6e2d5-d377-4e42-ac75-390ad4c206d9",
						"exec": [
							"function getRandomInt(min, max) {",
							"  return Math.floor(Math.random() * (max - min)) + min;",
							"}",
							"",
							"pm.sendRequest(\"http://easyquote/api/data/timezones\", function (err, response) {",
							"    let timezones = response.json();",
							"    pm.environment.set(\"timezone_id\", timezones[getRandomInt(0, timezones.length)].id);",
							"});",
							"",
							"pm.sendRequest(\"http://easyquote/api/data/countries\", function (err, response) {",
							"    let countries = response.json();",
							"    pm.environment.set(\"country_id\", countries[getRandomInt(0, countries.length)].id);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
